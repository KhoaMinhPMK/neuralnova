{
  "name": "neuralnova-file-server",
  "version": "1.0.0",
  "description": "NeuralNova File Upload Server - Handle image/video uploads with optimization",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:https": "node server-https.js",
    "dev": "nodemon server.js",
    "dev:https": "nodemon server-https.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:start:https": "pm2 start server-https.js --name neuralnova-file-server",
    "pm2:stop": "pm2 stop neuralnova-files",
    "pm2:restart": "pm2 restart neuralnova-files",
    "pm2:logs": "pm2 logs neuralnova-files"
  },
  "keywords": [
    "file-upload",
    "image-optimization",
    "express",
    "multer"
  ],
  "author": "NeuralNova",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}

